{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && cp dist/index.html dist/200.html",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint --cache --ext .ts,.tsx ./src",
    "lint:fix": "eslint --cache --fix --ext .ts,.tsx ./src",
    "format": "prettier --cache --check ./src/**/*.{ts,tsx}",
    "format:fix": "prettier --cache --write ./src/**/*.{ts,tsx}",
    "prepare": "husky install",
    "lint-staged": "lint-staged --no-stash",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "msw-storybook-addon": "^1.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-hooks": "^1.0.1",
    "react-icons": "^4.8.0",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.8.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/cli": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-vite": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-is": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.0",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.1",
    "msw": "^1.2.1",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix --ext .ts,.tsx",
      "prettier --cache --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
